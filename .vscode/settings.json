// Platzieren Sie Ihre Einstellungen in dieser Datei, um Standard- und Benutzereinstellungen zu Ã¼berschreiben.
{
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "files.exclude": {
    ".tmp": true,
    ".history": true,
    ".build/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/dist/": true,
    "packages/**/node_modules/": false,
    "stacks/**/.webpack": true,
    "stacks/**/.awcache": true,
    "stacks/**/node_modules/": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    ".serverless": true,
    "stacks/**/*.js": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "TodoParser": {
    "folderExclude": [".history", ".vscode", "node_modules", "dist", ".webpack"]
  }
}
